{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-microsoft-contrib",
      "tslint-immutable"
    ],
    "jsRules": {},
    "rules": {
      "no-import-side-effect": [true, {"ignore-module": "(\\.html|\\.css)$"}],
      "no-relative-imports": [false],
      "no-suspicious-comment": [false]
    },
    "rulesDirectory": []
}
